    "@tsconfig/node20": "^20.1.7",
    "axios": "^1.13.2",
    "bootstrap": "^5.3.8",
    "class-transformer": "^0.5.1",
    "element-plus": "^2.11.8",
    "normalize.css": "^8.0.1",
    "pinia": "^3.0.3",
    "reflect-metadata": "^0.2.2",
    "tiptap-extension-resize-image": "^1.3.2",
